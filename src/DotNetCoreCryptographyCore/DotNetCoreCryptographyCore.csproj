<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ProjectGuid>70F31DD8-AFD5-4353-B109-045481B8A93C</ProjectGuid>
    <PackageProjectUrl>https://github.com/alkampfergit/DotNetCoreCryptography</PackageProjectUrl>
    <Title>Dot Net Core Cryptograpy</Title>
    <Authors>alkampfer</Authors>
    <RepositoryUrl>https://github.com/alkampfergit/DotNetCoreCryptography</RepositoryUrl>
    <RepositoryType>git</RepositoryType>


    <!-- Generate symbol packages (.snupkg) -->
    <!-- You must publish both packages, the package that contains the DLL (.nupkg) and the one that contains the symbols (.snupkg) -->
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
